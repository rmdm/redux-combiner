{
  "name": "redux-combiner",
  "version": "0.4.4",
  "description": "Convenient reducers combiner for Redux",
  "main": "dest/combiner.js",
  "scripts": {
    "ci": "npm run lint && npm run cover && npm run karma",
    "test": "npm run lint && npm run mocha && npm run karma",
    "lint": "eslint src",
    "mocha": "mocha --recursive --require babel-register test",
    "karma": "karma start karma.conf.js",
    "cover": "cross-env NODE_ENV=test nyc npm run mocha",
    "compile": "babel --out-dir dest src",
    "prepare": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rmdm/redux-combiner.git"
  },
  "keywords": [
    "redux",
    "redux-combiner",
    "reducers",
    "combineReducers",
    "combine"
  ],
  "author": "rmdm",
  "license": "MIT",
  "devDependencies": {
    "@babel/polyfill": "^7.0.0-beta.46",
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^5.1.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.2",
    "coveralls": "^3.0.1",
    "cross-env": "^5.1.3",
    "eslint": "^5.16.0",
    "karma": "^4.1.0",
    "karma-browserify": "^6.0.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "mocha": "^6.1.4",
    "nyc": "^14.0.0",
    "redux": "^4.0.1",
    "redux-loop": "^4.2.4"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  },
  "files": [
    "dest/combiner.js"
  ]
}
